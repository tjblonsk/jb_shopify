	left: 0;
}
.touchcarousel .touchcarousel-wrapper {
	position: relative;
	overflow:hidden;
	width: 100%;

}





/******************************************************\
*
*  2. Carousel items (item styling, spacing between items)
*
\******************************************************/

.touchcarousel .touchcarousel-item {
	/* use margin-right for spacing between items */

	padding: 0;
	float: left;
}

/* Last carousel item  */
.touchcarousel .touchcarousel-item.last {
	margin-right: 0 !important;
}





/******************************************************\
*
*  3. Arrows(direction) navigation
*
\******************************************************/

/* arrow hitstate and icon holder */
.touchcarousel .arrow-holder {
	height: 100%;
	width: 45px;
	position: absolute;
	top: 0;
	display: block;
	cursor: pointer;
	z-index: 25;
}
.touchcarousel .arrow-holder.left {
	left: 0;
}
.touchcarousel .arrow-holder.right {
	right: 0;
}

/* arrow icons */
.touchcarousel .arrow-icon{
	width: 45px;
	height: 27px;
	top:50%;
	margin-top:-13.5px;
	position: absolute;
	cursor: pointer;
  opacity:.75;
}
.touchcarousel .arrow-icon.left {position:absolute;left:-25px;background:url({{'arrow_left.png'|asset_url}});background-repeat:no-repeat;}
.touchcarousel .arrow-icon.right {right: -53px;position:absolute;background:url({{'arrow_right.png'|asset_url}});background-repeat:no-repeat; }
.touchcarousel .arrow-holder:hover .arrow-icon {opacity:1; }
.touchcarousel .arrow-holder.disabled { cursor: default; }
.touchcarousel .arrow-holder.disabled .arrow-icon { cursor: default; }





/******************************************************\
*
*  4. Paging navigation
*
\******************************************************/

.touchcarousel .tc-paging-container {
	width:100%;
	overflow:hidden;
	position:absolute;
	margin-top:-20px;
	z-index:25;
}
.touchcarousel .tc-paging-centerer {
	float: left;
	position: relative;
	left: 50%;
}
.touchcarousel .tc-paging-centerer-inside {
	float: left;
	position: relative;
	left: -50%;
}

